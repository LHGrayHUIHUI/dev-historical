{
  "datacenter": "dc1",
  "data_dir": "/consul/data",
  "log_level": "INFO",
  "server": true,
  "bootstrap": true,
  "ui_config": {
    "enabled": true
  },
  "connect": {
    "enabled": true
  },
  "client_addr": "0.0.0.0",
  "retry_join": ["consul"],
  "services": [
    {
      "name": "consul",
      "tags": ["consul", "service-discovery"],
      "port": 8500,
      "check": {
        "http": "http://localhost:8500/v1/status/leader",
        "interval": "10s"
      }
    }
  ]
}